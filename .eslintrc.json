{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "prettier",
    "next",
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "globals": {
    "react": "readonly"
  },
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-unused-vars": "off",
    "no-console": "warn",
    "react/jsx-filename-extension": "off",
    "react/jsx-props-no-spreading": "off"
  }
}